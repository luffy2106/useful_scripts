   "source": [
    "## Delete similar files in folder"
   ]
  },
  {
   "cell_type": "raw",
   "metadata": {},
   "source": [
    "from pathlib import Path\n",
    "import glob \n",
    "\n",
    "all_files = set() \n",
    "my_path = \".\\\\Pictures\"\n",
    "files = glob.glob(my_path + '/**/*.*', recursive=True)  \n",
    "\n",
    "for file in files: \n",
    "    path_file = Path(file) \n",
    "    file_name = path_file.name \n",
    "    if file_name not in all_files: \n",
    "        all_files.add(file_name) \n",
    "    else: \n",
    "        rem_file = Path(file) \n",
    "        rem_file.unlink() "
   ]
  },
